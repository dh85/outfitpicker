{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "OutfitPicker (interactive)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/outfitpicker",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Run current folder (auto)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "env": {},
      "args": []
    },
    {
      "name": "Run current file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${fileDirname}",
      "cwd": "${fileDirname}",
      "env": {},
      "args": []
    },
    {
      "name": "Run cmd/outfitpicker",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/outfitpicker",
      "cwd": "${workspaceFolder}",
      "env": {},
      "args": []
    },
    {
      "name": "Test package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${workspaceFolder}",
      "cwd": "${workspaceFolder}",
      "args": []
    }
  ]
}